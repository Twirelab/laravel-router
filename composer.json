{
  "name": "twirelab/laravel-router",
  "description": "The new way of routing for the Laravel",
  "keywords": [
    "laravel",
    "routing",
    "router",
    "annotation",
    "twirelab"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0 | ^9.0 | ^10.0",
    "illuminate/routing": "^8.0 | ^9.0 | ^10.0"
  },
  "autoload": {
    "psr-4": {
      "Twirelab\\LaravelRouter\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.10"
  }
}
