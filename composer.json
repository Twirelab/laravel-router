{
  "name": "twirelab/laravel-router",
  "description": "The new way of routing for the Laravel",
  "keywords": [
    "laravel",
    "routing",
    "router",
    "annotation",
    "twirelab"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0 | ^9.0 | ^10.0",
    "illuminate/routing": "^8.0 | ^9.0 | ^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Twirelab\\LaravelRouter\\LaravelRouterServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Twirelab\\LaravelRouter\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "support": {
    "issues": "https://github.com/Twirelab/laravel-router/issues",
    "source": "https://github.com/Twirelab/laravel-router"
  }
}
